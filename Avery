import java.util.Random;
import java.util.Scanner;
import javax.swing.JOptionPane;

public class CoinFlippingProject {
	public static void main(String[] args) {

		String name = "";

		name = JOptionPane.showInputDialog("Place a bet! Type heads or tails!");

		JOptionPane.showMessageDialog(null, name + ", Good Luck!");

		Random rand = new Random();
		Scanner scan = new Scanner(System.in);
		int result = rand.nextInt(2);
		int heads = 0;
		int tails = 0;
		int bank = 10;
		int[] coinArray = new int[scan.nextInt()];
		for (int i = 0; i < coinArray.length; i++) {
			coinArray[i] = rand.nextInt(2);
			if (coinArray[i] == 0) {
				tails++;
			} else {
				heads++;
			}
			if (name == "heads")
			{
				bank++;
			}
				
		}
	}
}
